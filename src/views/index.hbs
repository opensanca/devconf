<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="utf-8">
  <title> {{{info.title}}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" type="image/x-icon" href="favicon.ico">
  <link rel="stylesheet" href="assets/css/bootstrap.min.css">
  <link rel="stylesheet" href="assets/css/styles.css">
  <link rel="stylesheet" href="assets/css/font-awesome-4.7.0/css/font-awesome.min.css">
  <!-- Open Graph -->
  <meta property="og:title" content="{{{info.title}}}" />
  <meta property="og:type" content="programação, docker, agile, javascript, mobile" />
  <meta property="og:site_name" content="{{{ info.eventName }}}" />
  <meta property="og:url" content="http://devconf.opensanca.com.br" />
  <meta property="og:image" content="http://opensanca.com.br/devconf/assets/img/Love-Coding.jpg" />
  <!-- IE polyfills, keep the order please -->
  <!-- <script src="../assets/js/modenizr-custom.js"></script> -->
</head>
<body>

    {{> header/index }}
    {{> section/hero }}
    {{> section/about }}

    {{#if speakers }}
        {{> section/speaker }}
    {{/if}}

    {{#if schedule }}
        {{> section/schedule }}
    {{else}}
      <section class="schedule" id="missing-schedule">
          <div class="wrapper">
              <h2 class="section-title">Programação</h2>
              <p>Em breve a programação</p>
          </div>
      </section>
    {{/if}}

    {{> section/sponsor }}

    {{#if partners}}
        {{> section/partner }}
    {{/if}}

    <section class="googleMaps" id="cd-google-map">
        <div id="google-container"></div>
        <div id="cd-zoom-in"></div>
        <div id="cd-zoom-out"></div>
        <address> {{ info.place }}| {{ info.address }}</address>
    </section>

    {{> section/contact }}
    {{> footer/index }}

    <div class="modal fade" id="speakersModal" tabindex="-1" aria-labelledby="speakersModal">
        <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="modalTitle"></h4>
            </div>
            <div class="modal-body" id="modalBody">
            </div>
            <div class="modal-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">Fechar</button>
            </div>
        </div>
        </div>
    </div>

    <script src="assets/js/jquery-1.12.4.min.js"></script>
    <script src="assets/js/bootstrap.min.js" async="true"></script>
    <script src="assets/js/main.js" async="true"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAiQ9MVvu3J-DL1uxMSempFbZNXlarEGs0"></script>
    <script src="{{{sympla.script}}}"></script>

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-96838079-2', 'auto');
        ga('send', 'pageview');

    </script>


</body>
</html>
